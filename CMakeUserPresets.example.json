{
  "version": 4,
  "configurePresets": [
    {
      "name": "user",
      "inherits": ["base", "platform-generator"],
      "cacheVariables": {
        "AUTO_DEPLOY": true,
        "GENERATE_AMXX_OFFSETS": false,
        "GENERATE_HAMDATA_INI": false
      }
    }
  ],
  "buildPresets": [
    {
      "name": "user-debug",
      "configurePreset": "user",
      "inheritConfigureEnvironment": true,
      "configuration": "Debug"
    },
    {
      "name": "user-release",
      "configurePreset": "user",
      "inheritConfigureEnvironment": true,
      "configuration": "RelWithDebInfo"
    }
  ]
}